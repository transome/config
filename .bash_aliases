alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'
alias grep='grep --color=auto --exclude-dir={bin,obj,packages,\.vs,node_modules} -nI'

alias ls='ls -p --color=auto --group-directories-first'
# alias ls='ls -p --group-directories-first'
alias ll='ls -1'
alias la='ls -1A'

alias gs='git status'
alias vs='explorer.exe *.sln'
alias gp='git pull'
